    "scripts": {
        "dll-dev":
            "cross-env NODE_ENV=development webpack --config build/webpack.dll.conf.js --progress && <% if(notWindows){ %>sudo<% } %> npm run dev",
        "dll-build":
            "cross-env NODE_ENV=production webpack --config build/webpack.dll.conf.js --progress && npm run build",
        "dev": "node build/dev-server.js",
        "start": "npm run dev",
        "build": "node build/build.js",
        "unit":
            "cross-env BABEL_ENV=test && export PHANTOMJS_BIN=/usr/local/lib/node_modules/phantomjs/lib/phantom/bin/phantomjs && ./node_modules/.bin/karma start test/unit/karma.conf.js --single-run",
        "e2e": "node test/e2e/runner.js",
        "test": "npm run unit && npm run e2e",
        "lint": "eslint --ext .js,.vue src test/unit/specs test/e2e/specs"
    },
